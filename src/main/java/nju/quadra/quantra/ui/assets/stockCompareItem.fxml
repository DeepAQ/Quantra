<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Font?>
<fx:root type="FlowPane" fx:id="flowCharts" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.121"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="labelName" alignment="CENTER" text="Label" underline="true" GridPane.columnIndex="0"
               GridPane.rowIndex="0">
            <font>
                <Font size="36.0"/>
            </font>
        </Label>
    </children>
</fx:root>
